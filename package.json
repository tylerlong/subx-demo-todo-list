{
  "name": "subx-demo-todomvc",
  "description": "React & SubX TodoMVC Example, SubX is the next generation state container",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --progress --colors",
    "release": "webpack --progress --colors --config webpack.config.prod.babel.js",
    "lint": "standard",
    "test": "jest -w 1",
    "upgradeSubX": "yarn remove subx react-subx && yarn add subx react-subx"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "director": "^1.2.8",
    "pluralize": "^7.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-subx": "^0.4.6",
    "rxjs": "^6.3.3",
    "subx": "^0.7.0",
    "todomvc-app-css": "^2.1.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "ramda": "^0.25.0",
    "react-test-renderer": "^16.6.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "timeout-as-promise": "^1.0.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-strip-block": "^0.2.0",
    "yarn-upgrade-all": "^0.5.0"
  },
  "standard": {
    "ignore": [
      "/docs/"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "babel-core"
    ]
  }
}
